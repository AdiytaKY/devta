{
  "name": "devta",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:start": "npx concurrently -k \"npm run serve:front\" \"npm run watch\"",
    "watch": "nodemon --exec npm run serve:electron",
    "serve:electron": "set APP_IS_DEV=yes && npm run build && wait-on tcp:3000 && electron .",
    "serve:front": "npm run dev",
    "electron:builder": "electron-builder",
    "electron:build": "set APP_IS_DEV=no && npm run build && npm run electron:builder"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@popperjs/core": "^2.11.8",
    "@vuepic/vue-datepicker": "^5.4.0",
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.19.2",
    "highlight.js": "^11.8.0",
    "jose": "^4.14.4",
    "js2xmlparser": "^5.0.0",
    "json-to-pretty-yaml": "^1.2.2",
    "jwt-decode": "^3.1.2",
    "marked": "^7.0.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nodemon": "^3.0.1",
    "pinia": "^2.1.6",
    "sql-formatter": "^12.2.4",
    "sql-query-identifier": "^2.5.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
